  <!-- SCRIPTS -->
  
  <!-- main script that will toggle all darkmode controls at once -->
  <script>
    function darkmode() {
      darkmodeToggle();
      darkmodeImage();
      darkmodeIcon();
      darkmodeTitle();
      darkmodeAni();
    }
  </script>
  
  <!-- darkmodeToggle: script that controls the darkmode toggle -->
  <script> 
    function darkmodeToggle() {
      var darkTog = document.body;
      darkTog.classList.toggle("darkmode");
    }
  </script>

  <!-- darkmodeImage: script that controls the image toggle -->
  <script>
    const avi = document.getElementById("avi");

    let isFirstImage = true;

    function darkmodeImage() {
    
avi.src="https://64.media.tumblr.com/be654af1f3a65476928d79675e0624a6/39761a2bc4fd830b-0a/s1280x1920/df050c2ae1b07328735e284ae88fcf5d2c0efe16.jpg"; /* img 2 (darkmode) */
    if (isFirstImage) {
    avi.src = "https://64.media.tumblr.com/be654af1f3a65476928d79675e0624a6/39761a2bc4fd830b-0a/s1280x1920/df050c2ae1b07328735e284ae88fcf5d2c0efe16.jpg"; /* img 2 (darkmode) */
  } else {
    avi.src = "https://64.media.tumblr.com/f40c9ebfe0d6d4aaf3a2b80d39d9fda0/ced969d3d5a2b01e-ec/s1280x1920/99878627d4eb4a723b6e263989e1a98df33c5426.png";
  }
  isFirstImage = !isFirstImage;
  }
  
  </script>


  <!-- darkmodeIcon: script that controls the icon toggle -->
  <script>
    const icon = document.getElementById("icon");
    let isFirstIcon = true;
    function darkmodeIcon() {
      icon.classList="fa-regular fa-sun"; /* img 2 (darkmode) */
      if (isFirstIcon) {
        icon.classList="fa-regular fa-sun"; /* img 2 (darkmode) */
      }
      else {
        icon.classList="fa-regular fa-moon";
      }
      isFirstIcon = !isFirstIcon;
    }
  </script>
  <!-- darkmodeAni: script that controls the icon animation -->
  <script>
    var x,y,n=0,ny=0,rotINT,rotYINT
      
    function darkmodeAni() {
      y=document.getElementById("icon")
        clearInterval(rotYINT)
          rotYINT=setInterval("darkmodeAniStart()",1)
    }
    function darkmodeAniStart() {
      ny=ny+1
      y.style.transform="rotateY(" + ny + "deg)"
      y.style.webkitTransform="rotateY(" + ny + "deg)"
      y.style.OTransform="rotateY(" + ny + "deg)"
      y.style.MozTransform="rotateY(" + ny + "deg)"
      if (ny==180 || ny>=540) 
      {
        clearInterval(rotYINT)
          if (ny>=540){ny=180}
      }
    }
</script>
  <!-- darkmodeTitle: script that controls the darkmode hover title -->
  <script>
    const title = document.getElementById("darkTitle");

    let isFirstTitle = true;

    function darkmodeTitle() {
    
title.title="light mode";
    if (isFirstTitle) {
    title.title="light mode";
  } else {
    title.title="dark mode";
  }
  isFirstTitle = !isFirstTitle;
  }
  
  </script>

  <!-- darkmodeSave: script that saves darkmode toggle -->
